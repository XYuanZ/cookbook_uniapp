#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aithen/Downloads/me-project/cookbook_uniapp/node_modules/.pnpm/tailwindcss@3.4.19_yaml@1.10.2/node_modules/tailwindcss/lib/node_modules:/Users/aithen/Downloads/me-project/cookbook_uniapp/node_modules/.pnpm/tailwindcss@3.4.19_yaml@1.10.2/node_modules/tailwindcss/node_modules:/Users/aithen/Downloads/me-project/cookbook_uniapp/node_modules/.pnpm/tailwindcss@3.4.19_yaml@1.10.2/node_modules:/Users/aithen/Downloads/me-project/cookbook_uniapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aithen/Downloads/me-project/cookbook_uniapp/node_modules/.pnpm/tailwindcss@3.4.19_yaml@1.10.2/node_modules/tailwindcss/lib/node_modules:/Users/aithen/Downloads/me-project/cookbook_uniapp/node_modules/.pnpm/tailwindcss@3.4.19_yaml@1.10.2/node_modules/tailwindcss/node_modules:/Users/aithen/Downloads/me-project/cookbook_uniapp/node_modules/.pnpm/tailwindcss@3.4.19_yaml@1.10.2/node_modules:/Users/aithen/Downloads/me-project/cookbook_uniapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
